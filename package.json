{
  "name": "psyrehab",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:https": "npm run setup-ssl && vite",
    "server": "node server.js",
    "server:dev": "nodemon server.js",
    "setup-ssl": "node scripts/setup-ssl.js",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer",
    "build:staging": "NODE_ENV=staging npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "echo 'E2E tests not configured yet'",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:all": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "serve": "vite preview --port 4173",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "ci": "npm run type-check && npm run lint && npm run test && npm run build",
    "deploy:preview": "npm run build && npm run serve",
    "security:audit": "node scripts/security-audit.cjs",
    "security:deps": "npm audit",
    "security:fix": "npm audit fix",
    "security:report": "npm run security:audit && echo 'Security report generated in security-reports/ directory'"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.49.10",
    "@tailwindcss/postcss": "^4.1.8",
    "@tanstack/react-query": "^5.80.5",
    "@types/recharts": "^1.8.29",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "connect-redis": "^8.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "lucide-react": "^0.513.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.15.3",
    "redis": "^5.5.6",
    "tailwind-merge": "^3.3.0",
    "task-master-ai": "^0.16.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tanstack/react-query-devtools": "^5.80.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "https": "^1.0.0",
    "jsdom": "^26.1.0",
    "mkcert": "^3.2.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.8",
    "terser": "^5.40.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.1"
  }
}
